#/bin/bash

if [ $1 ] ; then
	line=$1
else
	echo 'Enter the name of the round: '
	read line
fi

mkdir $line
git clone https://github.com/vladtarniceru/AutoCompiler-codeforces $line

cd $line
touch input
rm -r -f .new
echo $line > .roundName 

g++ -std=c++0x -Wall -o .getInput .getInput.cpp

for var in {A..E}; do
	cp sample.cpp $var.cpp
done


